{
  "name": "frontend",
  "version": "1.0.0",
  "type": "module",
  "license": "LICENSE",
  "author": {
    "name": "Eugen Hildt",
    "email": "eugen.hildt@litlbyte.com"
  },
  "bugs": {
    "email": "eugen.hildt@litlbyte.com"
  },
  "scripts": {
    "version": "npm --no-git-tag-version version",
    "ncu:update": "npx npm-check-updates --doctor -u",
    "ncu:validate": "npx npm-check-updates -e 2 --packageFile=./package.json",
    "dev": "vite",
    "build": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "depcheck": "npx depcheck .",
    "format": "prettier --write ./src",
    "lint": "eslint .",
    "lint-staged": "npx lint-staged --allow-empty",
    "ts-unused-exports": "npx ts-unused-exports ./tsconfig.exclude.json --maxIssues=20",
    "depcruiser": "npx depcruise -c .depcruiser.cjs --ts-config tsconfig.json src"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "classnames": "^2.3.3",
    "date-fns": "^3.0.6",
    "i18next": "^23.7.11",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-chained-backend": "^4.6.2",
    "i18next-http-backend": "^2.4.2",
    "i18next-localstorage-backend": "^4.2.0",
    "libs": "^1.0.0",
    "nanoid": "^5.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-icons": "^4.12.0",
    "ui": "^1.0.0",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "latest",
    "autoprefixer": "^10.4.16",
    "css-reset-and-normalize": "^2.3.6",
    "depcheck": "^1.4.7",
    "dependency-cruiser": "^15.5.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jsdom": "latest",
    "lint-staged": "^15.2.0",
    "npm-check-updates": "^16.14.12",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prop-types": "^15.8.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.69.5",
    "ts-unused-exports": "^10.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.1.0"
  },
  "stackblitz": {
    "startCommand": "npm run test:ui"
  }
}