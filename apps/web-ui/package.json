{
  "name": "@acap/web-ui",
  "version": "0.24.10",
  "type": "module",
  "license": "LICENSE",
  "author": {
    "name": "Eugen Hildt",
    "email": "eugen.hildt@litlbyte.com"
  },
  "bugs": {
    "email": "eugen.hildt@litlbyte.com"
  },
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "version": "npm --no-git-tag-version version",
    "ncu:update": "npx npm-check-updates --interactive --format group",
    "ncu:validate": "npx npm-check-updates -e 2 --packageFile=./package.json",
    "start:dev": "vite --host",
    "build": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "depcheck": "npx depcheck .",
    "format": "prettier --write ./src",
    "lint": "eslint --config eslint.config.mjs src",
    "lint-staged": "npx lint-staged --allow-empty",
    "ts-unused-exports": "npx ts-unused-exports ./tsconfig.exclude.json --maxIssues=20",
    "depcruiser": "npx depcruise -c .depcruiser.mjs --ts-config tsconfig.json src"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "bytes": "^3.1.2",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "i18next": "^23.15.1",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-chained-backend": "^4.6.2",
    "i18next-http-backend": "^2.6.1",
    "i18next-localstorage-backend": "^4.2.0",
    "immer": "^10.1.1",
    "nanoid": "^5.0.7",
    "react": "^18.3.1",
    "react-d3-tree": "^3.6.2",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.2",
    "react-icons": "^5.3.0",
    "react-pdf": "^9.1.0",
    "react-player": "^2.16.0",
    "swr": "^2.2.5",
    "url": "^0.11.4",
    "yaml": "^2.5.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^2.0.2",
    "@eslint/js": "^9.10.0",
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-interactions": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/addon-onboarding": "^8.3.0",
    "@storybook/blocks": "^8.3.0",
    "@storybook/manager-api": "^8.3.0",
    "@storybook/react": "^8.3.0",
    "@storybook/react-vite": "^8.3.0",
    "@storybook/theming": "^8.3.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bytes": "^3.1.4",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "latest",
    "autoprefixer": "^10.4.20",
    "css-reset-and-normalize": "^2.3.6",
    "depcheck": "^1.4.7",
    "dependency-cruiser": "^16.4.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "2.0.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "jsdom": "latest",
    "lint-staged": "^15.2.10",
    "npm-check-updates": "^17.1.1",
    "postcss": "^8.4.47",
    "prop-types": "^15.8.1",
    "react-refresh": "^0.14.2",
    "sass": "^1.78.0",
    "storybook": "^8.3.0",
    "ts-unused-exports": "^10.1.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0",
    "vite": "^5.4.5",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1"
  },
  "stackblitz": {
    "startCommand": "npm run test:ui"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "packageManager": "pnpm@9.10.0"
}