{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": true,
    "assets": [
      {
        "include": "configs/config-yml/config.yml",
        "outDir": "dist/configs/config-yml/config.yml"
      }
    ]
  }
}