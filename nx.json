{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "build:prod",
          "depcheck",
          "depcruiser",
          "licensee",
          "lint",
          "lint-staged",
          "test",
          "build-storybook",
          "ncu:update"
        ]
      }
    }
  },
  "targetDefaults": {
    "ncu:update": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "depcheck": {
      "dependsOn": [
        "^depcheck"
      ]
    },
    "depcruiser": {
      "dependsOn": [
        "^depcruiser"
      ]
    },
    "licensee": {
      "dependsOn": [
        "^licensee"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "lint-staged": {
      "dependsOn": [
        "^lint-staged"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "build": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "build:prod": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    }
  }
}
