{
  "name": "@cultify/config-manager",
  "version": "0.5.20",
  "description": "A simple and convenient way to config your apps ;)",
  "author": "Eugen Hildt",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build:prod": "nest build --path ./tsconfig.build.prod.json",
    "build": "nest build",
    "check-license": "licensee --errors-only",
    "dependency-cruiser:graph": "npx depcruise -c .dependency-cruiser.js -x \".+.spec.ts\" -T dot ./src | dot -T png > .dependency-graph.png",
    "dependency-cruiser": "npx depcruise -c .dependency-cruiser.js --ts-config tsconfig.json src",
    "format": "prettier --write .",
    "gitleaks": "docker run --rm -v $(cd .. && pwd):/app zricethezav/gitleaks -c /app/${PWD##*/}/gitleaks.toml detect -v --source=\"/app/${PWD##*/}/\" --no-git",
    "lint-staged": "npx lint-staged --allow-empty",
    "lint": "eslint .",
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "start:debug": "nest start --debug --watch  --preserveWatchOutput",
    "start:dev": "dotenv -e ./env/defaults.env -- nest start --watch  --preserveWatchOutput",
    "start:prod": "node dist/main",
    "start": "nest start",
    "test:clearCache": "jest --clearCache",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e ./env/defaults.env -- jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "test": "jest --passWithNoTests",
    "ts-unused-exports": "npx ts-unused-exports ./tsconfig.json",
    "ncu:update": "npx npm-check-updates --doctor -u",
    "ncu:validate": "npx npm-check-updates -e 2 --packageFile=./package.json"
  },
  "dependencies": {
    "@fastify/static": "^6.5.0",
    "@nestjs/common": "9.1.4",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.1.4",
    "@nestjs/microservices": "9.1.4",
    "@nestjs/mongoose": "9.2.0",
    "@nestjs/platform-fastify": "^9.1.4",
    "@nestjs/swagger": "^6.1.2",
    "cache-manager": "5.0.0",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "mongoose": "6.6.5",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "short-unique-id": "4.4.4"
  },
  "devDependencies": {
    "@jest/types": "29.1.2",
    "@nestjs/cli": "9.1.4",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.1.4",
    "@types/cache-manager": "4.0.2",
    "@types/cache-manager-ioredis": "2.0.2",
    "@types/eslint": "8.4.6",
    "@types/jest": "29.1.2",
    "@types/node": "18.8.3",
    "@types/prettier": "2.7.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "dependency-cruiser": "11.16.1",
    "dotenv-cli": "6.0.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-no-loops": "0.3.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sonarjs": "0.15.0",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "jest-extended": "3.1.0",
    "jest-junit": "14.0.1",
    "licensee": "9.0.0",
    "lint-staged": "13.0.3",
    "npm-check-updates": "16.3.9",
    "prettier": "2.7.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.0",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "ts-unused-exports": "8.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  }
}
