{
  "name": "@cultify/config-manager",
  "version": "0.5.18",
  "description": "A simple and convenient way to config your apps ;)",
  "author": "Eugen Hildt",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build:prod": "nest build --path ./tsconfig.build.prod.json",
    "build": "nest build",
    "ci:build": "npm build",
    "ci:install:dev:frozen": "npm install --non-interactive --frozen-lockfile",
    "ci:install:dev": "npm install --non-interactive",
    "ci:install:prod:frozen": "npm install --non-interactive --frozen-lockfile --production",
    "ci:install:prod": "npm install --non-interactive --production",
    "ci:license:report": "licensee --ndjson > license-report.json",
    "ci:license": "licensee --errors-only",
    "ci:lint": "npm lint",
    "ci:outdated:report": "npm outdated --json > outdated-report.json",
    "ci:outdated": "npm outdated",
    "ci:semantic-release": "semantic-release",
    "ci:test": "jest --ci --reporters=default --reporters=jest-junit --passWithNoTests",
    "ci:test:coverage": "jest --coverage --ci --reporters=default --reporters=jest-junit --passWithNoTests",
    "ci:vulnerabilities:report": "npm audit --json > vulnerabilities-report.json",
    "ci:vulnerabilities": "npm audit",
    "clinic:bubble": "dotenv -e ./env/clinic.env -- npx clinic bubbleprof -- node ./dist/main.js",
    "clinic:clean": "npx clinic clean",
    "clinic:doctor": "dotenv -e ./env/clinic.env -- npx clinic doctor -- node ./dist/main.js",
    "clinic:flame": "dotenv -e ./env/clinic.env -- npx clinic flame -- node ./dist/main.js",
    "compose:up": "docker compose up -d mongo redis && docker compose up config-manager",
    "dependency-cruiser:graph": "npx depcruise -c .dependency-cruiser.js -x \".+.spec.ts\" -T dot ./src | dot -T png > .dependency-graph.png",
    "dependency-cruiser": "npx depcruise -c .dependency-cruiser.js --ts-config tsconfig.json src",
    "docker:build:dev": "docker build --rm --target dev -t localhost:5000/config-manager .",
    "docker:build:prod": "docker build --rm --target prod -t docker.hub.ideea.app.corpintra.net/eeagle/services/config-manager .",
    "docker:build": "docker build --rm --target prod -t localhost:5000/config-manager .",
    "docker:login": "docker login docker.hub.ideea.app.corpintra.net",
    "docker:publish:local": "docker push localhost:5000/config-manager",
    "docker:publish:remote": "docker push docker.hub.ideea.app.corpintra.net/eeagle/services/config-manager",
    "docker:tag": "docker image tag localhost:5000/config-manager docker.hub.ideea.app.corpintra.net/eeagle/services/config-manager",
    "format": "prettier --write .",
    "gitleaks": "docker run --rm -v $(cd .. && pwd):/app zricethezav/gitleaks -c /app/${PWD##*/}/gitleaks.toml detect -v --source=\"/app/${PWD##*/}/\" --no-git",
    "lint-staged": "npx lint-staged --allow-empty",
    "lint": "eslint .",
    "ncu:patch": "npx npm-check-updates -t patch --packageFile=./package.json",
    "ncu:update": "npx npm-check-updates --doctor -u ",
    "ncu:validate": "npx npm-check-updates -e 2 --packageFile=./package.json",
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "start:debug": "nest start --debug --watch  --preserveWatchOutput",
    "start:dev": "dotenv -e ./env/defaults.env -- nest start --watch  --preserveWatchOutput",
    "start:prod": "node dist/main",
    "start": "nest start",
    "test:clearCache": "jest --clearCache",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e ./env/defaults.env -- jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "test": "jest --passWithNoTests",
    "ts-unused-exports": "npx ts-unused-exports ./tsconfig.json",
    "upgrade-latest": "npm upgrade-interactive --latest"
  },
  "dependencies": {
    "@nestjs/common": "9.1.2",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.1.2",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/microservices": "9.1.2",
    "@nestjs/mongoose": "9.2.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.1.2",
    "@nestjs/swagger": "6.1.2",
    "argon2": "0.29.1",
    "cache-manager": "4.1.0",
    "cache-manager-ioredis": "2.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "ioredis": "5.2.3",
    "mongoose": "6.4.4",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "short-unique-id": "4.4.4",
    "swagger-ui-express": "4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@jest/types": "29.0.3",
    "@nestjs/cli": "9.1.3",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.1.2",
    "@saithodev/semantic-release-backmerge": "2.1.2",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "semantic-release/commit-analyzer",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/gitlab": "9.4.2",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/cache-manager": "4.0.2",
    "@types/cache-manager-ioredis": "2.0.2",
    "@types/compression": "1.7.2",
    "@types/eslint": "8.4.6",
    "@types/express": "4.17.14",
    "@types/jest": "29.0.3",
    "@types/node": "18.7.23",
    "@types/passport-jwt": "3.0.6",
    "@types/prettier": "2.7.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "clinic": "12.0.0",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "dependency-cruiser": "11.16.1",
    "dotenv-cli": "6.0.0",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-no-loops": "0.3.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sonarjs": "0.15.0",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "jest-extended": "3.1.0",
    "jest-junit": "14.0.1",
    "licensee": "9.0.0",
    "lint-staged": "13.0.3",
    "npm-check-updates": "16.3.2",
    "prettier": "2.7.1",
    "semantic-release": "19.0.5",
    "source-map-support": "0.5.21",
    "supertest": "6.2.4",
    "ts-jest": "29.0.2",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "ts-unused-exports": "8.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.3"
  }
}
