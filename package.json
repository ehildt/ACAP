{
  "name": "@litlbyte/acap",
  "version": "0.9.6",
  "description": "A simple and convenient way to distribute and provide content to your apps ;)",
  "repository": "git@github.com:ehildt/acap.git",
  "author": {
    "name": "Eugen Hildt",
    "email": "eugen.hildt@litlbyte.com"
  },
  "license": "MIT",
  "bugs": {
    "email": "eugen.hildt@litlbyte.com"
  },
  "scripts": {
    "build:prod": "nest build --path ./tsconfig.build.prod.json",
    "build": "nest build",
    "licensee": "licensee --errors-only",
    "format": "prettier --write .",
    "gitleaks": "docker run --rm -v $(cd .. && pwd):/app zricethezav/gitleaks -c /app/${PWD##*/}/gitleaks.toml detect -v --source=\"/app/${PWD##*/}/\" --no-git",
    "lint-staged": "npx lint-staged --allow-empty",
    "lint": "eslint .",
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "start:debug": "nest start --debug --watch  --preserveWatchOutput",
    "start:dev": "dotenv -e ./env/defaults.env -- nest start --watch  --preserveWatchOutput",
    "start:node": "node dist/main",
    "start": "nest start",
    "test:clearCache": "jest --clearCache",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e ./env/defaults.env -- jest --detectOpenHandles --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "test": "jest --passWithNoTests",
    "ts-unused-exports": "npx ts-unused-exports ./tsconfig.json",
    "ncu:update": "npx npm-check-updates --doctor -u",
    "ncu:validate": "npx npm-check-updates -e 2 --packageFile=./package.json",
    "depcruiser": "npx depcruise -c .depcruiser.cjs --ts-config tsconfig.json src",
    "depcruiser:graph": "npx depcruise -c .depcruiser.cjs -x \".+.spec.ts\" -T dot ./src | dot -T png > .depcruiser-graph.png",
    "depcheck": "npx depcheck ."
  },
  "dependencies": {
    "@fastify/multipart": "^7.7.3",
    "@fastify/static": "^6.10.2",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "10.1.3",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.1.3",
    "@nestjs/microservices": "10.1.3",
    "@nestjs/mongoose": "10.0.1",
    "@nestjs/platform-fastify": "^10.1.3",
    "@nestjs/swagger": "^7.1.8",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "bullmq": "^4.7.4",
    "cache-manager": "5.2.3",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "fastify": "4.21",
    "joi": "^17.9.2",
    "js-yaml": "^4.1.0",
    "mongoose": "7.4.3",
    "mqtt": "^5.0.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "short-unique-id": "4.4.4"
  },
  "devDependencies": {
    "@jest/types": "29.6.1",
    "@nestjs/cli": "10.1.12",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.1.3",
    "@types/cache-manager": "4.0.2",
    "@types/cache-manager-ioredis": "2.0.3",
    "@types/eslint": "8.44.2",
    "@types/jest": "29.5.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "20.5.1",
    "@types/prettier": "2.7.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "depcheck": "^1.4.3",
    "dependency-cruiser": "13.1.4",
    "dotenv-cli": "7.3.0",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-no-loops": "0.3.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "0.20.0",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "jest-extended": "4.0.1",
    "jest-junit": "16.0.0",
    "licensee": "10.0.0",
    "lint-staged": "14.0.0",
    "npm-check-updates": "16.12.2",
    "prettier": "^3.0.2",
    "source-map-support": "0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "ts-unused-exports": "10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  }
}
