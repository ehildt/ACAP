{
  "name": "components",
  "version": "1.0.0",
  "type": "module",
  "license": "LICENSE",
  "main": "src/index.js",
  "author": {
    "name": "Eugen Hildt",
    "email": "eugen.hildt@litlbyte.com"
  },
  "bugs": {
    "email": "eugen.hildt@litlbyte.com"
  },
  "scripts": {
    "ncu:update": "npx npm-check-updates --doctor -u",
    "ncu:validate": "npx npm-check-updates -e 2 --packageFile=./package.json",
    "build": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "depcheck": "npx depcheck .",
    "format": "prettier --write ./src",
    "licensee": "licensee --errors-only",
    "lint": "eslint .",
    "lint-staged": "npx lint-staged --allow-empty",
    "ts-unused-exports": "npx ts-unused-exports ./tsconfig.exclude.json",
    "depcruiser": "npx depcruise -c .depcruiser.cjs --ts-config tsconfig.json src"
  },
  "peerDependencies": {
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-icons": "^4.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "latest",
    "autoprefixer": "^10.4.16",
    "css-reset-and-normalize": "^2.3.6",
    "depcheck": "^1.4.6",
    "dependency-cruiser": "^14.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "0.21.0",
    "eslint-plugin-storybook": "^0.6.14",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jsdom": "latest",
    "licensee": "^10.0.0",
    "lint-staged": "^14.0.1",
    "npm-check-updates": "^16.14.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.69.0",
    "ts-unused-exports": "^10.0.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "stackblitz": {
    "startCommand": "npm run test:ui"
  }
}