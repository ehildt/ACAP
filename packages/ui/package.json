{
  "name": "ui",
  "version": "1.0.0",
  "type": "module",
  "license": "LICENSE",
  "main": "src/index.ts",
  "author": {
    "name": "Eugen Hildt",
    "email": "eugen.hildt@litlbyte.com"
  },
  "bugs": {
    "email": "eugen.hildt@litlbyte.com"
  },
  "scripts": {
    "ncu:update": "npx npm-check-updates --doctor -u",
    "ncu:validate": "npx npm-check-updates -e 2 --packageFile=./package.json",
    "build": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "depcheck": "npx depcheck .",
    "format": "prettier --write ./src",
    "lint": "eslint ./src",
    "lint-staged": "npx lint-staged --allow-empty",
    "ts-unused-exports": "npx ts-unused-exports ./tsconfig.exclude.json --excludePathsFromReport=src/index.ts",
    "depcruiser": "npx depcruise -c .depcruiser.cjs --ts-config tsconfig.json src"
  },
  "peerDependencies": {
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-icons": "^4.11.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/addon-links": "^7.6.0",
    "@storybook/blocks": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "@storybook/react-vite": "^7.6.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "latest",
    "autoprefixer": "^10.4.16",
    "css-reset-and-normalize": "^2.3.6",
    "depcheck": "^1.4.7",
    "dependency-cruiser": "^15.5.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jsdom": "latest",
    "lint-staged": "^15.1.0",
    "npm-check-updates": "^16.14.11",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prop-types": "^15.8.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.69.5",
    "storybook": "^7.6.0",
    "ts-unused-exports": "^10.0.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "stackblitz": {
    "startCommand": "npm run test:ui"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "libs": "^1.0.0",
    "react-pdf": "^7.5.1"
  }
}