{
  "name": "ui",
  "version": "1.0.0",
  "type": "module",
  "license": "LICENSE",
  "main": "src/index.ts",
  "author": {
    "name": "Eugen Hildt",
    "email": "eugen.hildt@litlbyte.com"
  },
  "bugs": {
    "email": "eugen.hildt@litlbyte.com"
  },
  "scripts": {
    "ncu:update": "npx npm-check-updates --doctor -u",
    "ncu:validate": "npx npm-check-updates -e 2 --packageFile=./package.json",
    "build": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "depcheck": "npx depcheck .",
    "format": "prettier --write ./src",
    "lint": "eslint .",
    "lint-staged": "npx lint-staged --allow-empty",
    "ts-unused-exports": "npx ts-unused-exports ./tsconfig.exclude.json --excludePathsFromReport=src/index.ts",
    "depcruiser": "npx depcruise -c .depcruiser.cjs --ts-config tsconfig.json src"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.0.6",
    "@storybook/addon-interactions": "^8.0.6",
    "@storybook/addon-links": "^8.0.6",
    "@storybook/blocks": "^8.0.6",
    "@storybook/manager-api": "^8.0.6",
    "@storybook/react": "^8.0.6",
    "@storybook/react-vite": "^8.0.6",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^8.0.6",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "latest",
    "autoprefixer": "^10.4.19",
    "css-reset-and-normalize": "^2.3.6",
    "depcheck": "^1.4.7",
    "dependency-cruiser": "^16.3.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sonarjs": "0.25.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jsdom": "latest",
    "lint-staged": "^15.2.2",
    "npm-check-updates": "^16.14.18",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "react-d3-tree": "^3.6.2",
    "react-refresh": "^0.14.0",
    "sass": "^1.74.1",
    "storybook": "^8.0.6",
    "ts-unused-exports": "^10.0.1",
    "typescript": "^5.4.4",
    "vite": "^5.2.8",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  },
  "stackblitz": {
    "startCommand": "npm run test:ui"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "classnames": "^2.5.1",
    "libs": "^1.0.0",
